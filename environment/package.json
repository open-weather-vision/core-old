{
  "name": "owvision-environment",
  "version": "1.2.2",
  "description": "Common types and structures used across all owvision-packages.",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./interfaces": {
      "types": "./dist/interfaces/index.d.ts",
      "import": "./dist/interfaces/index.js"
    },
    "./scheduler":{
      "types":  "./dist/scheduler/index.d.ts",
      "import":  "./dist/scheduler/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./units": {
      "types": "./dist/units/index.d.ts",
      "import": "./dist/units/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "start": "cd ./dist && node ",
    "build": "npx tsc",
    "dev": "npx tsc --watch"
  },
  "keywords": [
    "owvision"
  ],
  "author": "Henri Hollmann",
  "license": "ISC",
  "devDependencies": {
    "@types/long-timeout": "^0.1.2",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.11",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@vinejs/vine": "^2.1.0",
    "long-timeout": "^0.1.1",
    "luxon": "^3.4.4",
    "simple-units": "^1.0.6",
    "vantjs": "^2.0.2"
  }
}
