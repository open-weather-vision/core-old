		"exit-hook": "^4.0.0",
		"ink": "^4.1.0",
		"ink-spinner": "^5.0.0",
		"jsonfile": "^6.1.0",
		"luxon": "^3.4.4",
		"meow": "^11.0.0",
		"owvision-environment": "^1.2.28",
		"react": "^18.2.0",
		"winston": "^3.13.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/clone": "^2.1.4",
		"@types/jsonfile": "^6.1.4",
		"@types/luxon": "^3.4.2",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
