{
  "name": "api",
  "version": "0.0.0",
  "description": "owvision's rest api",
  "private": true,
  "type": "module",
  "license": "ISC",
  "author": "Henri Hollmann (harrydehix)",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build --ignore-ts-errors",
    "fresh": "node ace migration:fresh --force --seed",
    "dev": "node ace serve --watch",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "migrate": "node ace migration:run --force",
    "build-image": "docker build -t owvision-api ."
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.6.1",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/api-client": "^2.0.3",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.5.7",
    "@types/adm-zip": "^0.5.5",
    "@types/long-timeout": "^0.1.2",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.12",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "hot-hook": "^0.2.6",
    "pino-pretty": "^11.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.1",
    "@adonisjs/core": "^6.9.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^20.6.0",
    "@datastructures-js/queue": "^4.2.3",
    "@vinejs/vine": "^2.0.0",
    "await-exec": "^0.1.2",
    "axios": "^1.7.2",
    "extract-zip": "^2.0.1",
    "long-timeout": "^0.1.1",
    "luxon": "^3.4.4",
    "owvision-environment": "^1.0.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "simple-units": "^1.0.6",
    "zip-a-folder": "^3.1.7"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config"
}
